@model IEnumerable<Employee_Chat.Models.Employee>

<h2>All Employees</h2>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Department</th>
            <th>Position</th>
            <th>Salary</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Name</td>
                <td>@employee.Department</td>
                <td>@employee.Position</td>
                <td>@employee.Salary</td>
            </tr>
        }
    </tbody>
</table>
